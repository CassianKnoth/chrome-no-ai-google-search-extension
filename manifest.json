{
	"manifest_version": 3,
	"name": "chrome-no-ai-google-search-extension",
	"version": "1.0.0",
	"description": "Appends '-ai' to Google search queries",
	"permissions": ["tabs"],
	"host_permissions": ["https://www.google.com/*"],
	"content_scripts": [
		{
			"matches": ["https://www.google.com/*"],
			"js": ["content.js"],
			"run_at": "document_start"
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}
