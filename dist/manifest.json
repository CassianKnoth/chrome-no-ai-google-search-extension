{
	"manifest_version": 3,
	"name": "No Ai Google Search",
	"version": "1.0.0",
	"description": "Appends '-AI' to Google search queries",
	"permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestWithHostAccess",
		"scripting"
	],
	"host_permissions": ["https://www.google.com/search*"],
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	}
}
