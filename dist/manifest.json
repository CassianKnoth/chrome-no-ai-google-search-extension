{
	"manifest_version": 3,
	"name": "No Ai Google Search",
	"version": "1.0.0",
	"description": "Appends '-AI' to Google search queries",
	"host_permissions": ["https://www.google.com/*"],
	"content_scripts": [
		{
			"matches": ["https://www.google.com/search*"],
			"js": ["content.js"],
			"run_at": "document_start"
		}
	]
}
